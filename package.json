{
  "name": "md-specgen",
  "version": "1.1.3",
  "type": "module",
  "description": "A flexible Markdown documentation generator with HTML/PDF output and AI-powered enhancements",
  "keywords": [
    "markdown",
    "documentation",
    "pdf",
    "html",
    "generator",
    "mermaid",
    "claude",
    "llm",
    "ai"
  ],
  "author": "takemi-ohama",
  "license": "MIT",
  "homepage": "https://github.com/takemi-ohama/md-specgen#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/takemi-ohama/md-specgen.git"
  },
  "bugs": {
    "url": "https://github.com/takemi-ohama/md-specgen/issues"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "md-specgen": "dist/cli/index.js"
  },
  "files": [
    "dist",
    "README.md",
    "README.ja.md",
    "CHANGELOG.md",
    "CHANGELOG.ja.md",
    "CONTRIBUTING.md",
    "CONTRIBUTING.ja.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepublishOnly": "npm test && npm run build",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.38.0",
    "@aws-sdk/client-bedrock-runtime": "^3.712.0",
    "@types/markdown-it": "^14.1.2",
    "@types/markdown-it-anchor": "^4.0.4",
    "@types/markdown-it-container": "^2.0.11",
    "chalk": "^5.4.1",
    "chokidar": "^4.0.3",
    "commander": "^12.1.0",
    "fs-extra": "^11.2.0",
    "glob": "^11.0.0",
    "gray-matter": "^4.0.3",
    "highlight.js": "^11.11.1",
    "js-yaml": "^4.1.0",
    "markdown-it": "^14.1.0",
    "markdown-it-anchor": "^9.2.0",
    "markdown-it-container": "^4.0.0",
    "markdown-it-include": "^2.0.0",
    "mermaid": "^11.4.0",
    "ora": "^8.1.1",
    "plantuml-encoder": "^1.4.0",
    "puppeteer": "^23.11.1"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^29.5.14",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^22.10.2",
    "@typescript-eslint/eslint-plugin": "^8.18.1",
    "@typescript-eslint/parser": "^8.18.1",
    "eslint": "^9.17.0",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "ts-jest": "^29.2.5",
    "typescript": "^5.7.2"
  }
}
